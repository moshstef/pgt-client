<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
    <settings>
        <!-- changes from the defaults -->
        <setting name="lazyLoadingEnabled" value="false" />
    </settings>
    <typeAliases>
        <typeAlias type="pgtest.security.domain.User" alias="user"/>
        <typeAlias type="pgtest.security.domain.Role" alias="role"/>
        <typeAlias type="pgtest.security.domain.Authority" alias="authority"/>
        <typeAlias type="pgtest.audit.domain.AuditTrace" alias="auditTrace"/>
    </typeAliases>
</configuration>